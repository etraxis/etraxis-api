{
    "name": "etraxis/etraxis",
    "description": "Issue tracking system with customizable workflows.",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "support": {
        "forum": "https://gitter.im/etraxis",
        "issues": "https://github.com/etraxis/etraxis/issues",
        "wiki": "https://github.com/etraxis/etraxis/wiki",
        "source": "https://github.com/etraxis/etraxis"
    },
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/translation": "5.0.*",
        "symfony/twig-pack": "^1.0",
        "symfony/yaml": "5.0.*",
        "webinarium/php-dictionary": "^1.0",
        "webinarium/php-properties": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/thanks": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "eTraxis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "eTraxis\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
